<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hexoskin.app.maps.MapsActivity">

    <TableLayout
        android:id="@+id/tableLayoutMaps"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:stretchColumns="5"
        android:visibility="gone">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="8dp"
                android:textColor="#000000" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="30dp"
                android:maxWidth="200dp"
                android:minWidth="200dp"
                android:paddingLeft="10dp"
                android:text="Distance"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/textViewDistance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:text="0 m"
                android:textColor="#000000"
                android:textSize="17dp"
                android:typeface="monospace" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textViewTimeElapsed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="30dp"
                android:paddingLeft="10dp"
                android:text="Temps"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />

            <Chronometer
                android:id="@+id/chronometer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:textColor="#000000"
                android:textSize="17dp"
                android:typeface="monospace" />
        </TableRow>


        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="30dp"
                android:paddingLeft="10dp"
                android:text="Calories"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/textViewCaloriesBurnedMaps"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:text="0 ca"
                android:textColor="#000000"
                android:textSize="17dp"
                android:typeface="monospace" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="30dp"
                android:paddingLeft="10dp"
                android:text="Moy. mÃ¨tre/min"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/textViewMoyMinKm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:height="30dp"
                android:text="0 m/min"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="30dp"
                android:paddingLeft="10dp"
                android:text="Vitesse"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />

            <TextView
                android:id="@+id/textViewSpeed"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:height="30dp"
                android:text="0 km/h"
                android:textColor="#000000"
                android:textSize="18dp"
                android:typeface="monospace" />
        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="10dp" />

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#805882FA">

            <ImageButton
                android:id="@+id/buttonPlay"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="2dp"
                android:background="@drawable/buttonstyle"
                android:maxWidth="40dp"
                android:minHeight="40dp"
                android:minWidth="40dp"
                android:src="@android:drawable/ic_media_play"
                android:visibility="gone" />

            <ImageButton
                android:id="@+id/buttonPause"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="1"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:background="@drawable/buttonstyle"
                android:maxWidth="40dp"
                android:minHeight="40dp"
                android:minWidth="40dp"
                android:src="@android:drawable/ic_media_pause"
                android:visibility="gone" />

            <ImageButton
                android:id="@+id/buttonStop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="2"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="8dp"
                android:background="@drawable/buttonstyle"
                android:maxWidth="40dp"
                android:minHeight="40dp"
                android:minWidth="40dp"
                android:src="@drawable/buttonstop1"
                android:visibility="gone" />

        </TableRow>

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_column="0"
                android:height="150dp" />

        </TableRow>

    </TableLayout>

    <TableLayout
        android:id="@+id/tableLayoutSearchMaps"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <ProgressBar
                android:id="@+id/progressBar1"
                style="?android:attr/progressBarStyleLarge"
                android:layout_column="0"
                android:layout_marginLeft="115dp"
                android:layout_marginTop="250dp"
                android:indeterminate="false"
                android:max="100"
                android:progress="1" />


        </TableRow>


        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <TextView
                android:id="@+id/textViewSearch"
                android:layout_column="0"
                android:layout_marginLeft="125dp"
                android:text="Searching.."
                android:textColor="#E40000"
                android:textSize="20dp"
                android:typeface="monospace" />

        </TableRow>

    </TableLayout>


</fragment>

